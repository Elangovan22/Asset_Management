<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editCaption"
        class="com.company.ams.web.assetallocated.AssetAllocatedEdit"
        datasource="assetAllocatedDs"
        focusComponent="fieldGroup"
        messagesPack="com.company.ams.web.assetallocated">
    <dsContext>
        <datasource id="assetAllocatedDs"
                    class="com.company.ams.entity.AssetAllocated"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="assetAllocatedDs">
            <column width="250px">
                <field id="user"/>
                <field id="assetid"/>
                <field id="company"/>
                <field id="modelname"/>
                <field id="quantity"/>
                <field id="name"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
